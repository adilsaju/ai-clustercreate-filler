{
  "manifest_version": 2,
  "name": "AI Filler",
 "description": "AI based extension",
 "version": "1.0",
 "content_scripts": [
  {
   
  "matches": ["http://localhost:5000/launch_instance"],
   "js": ["content.js"],
   "run_at": "document_end"
  }
 ],


  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Automatic Fill for cluster create",
    "default_popup": "popup.html"
  },

"permissions": [
  "tabs",
  "http://*/*",
  "https://*/*"
]

 }
